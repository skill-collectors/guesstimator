{
  "name": "guesstimator-frontend",
  "version": "0.0.1",
  "scripts": {
    "check-all": "npm run check && npm run lint && npm run test:unit && npm run test:e2e",
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "lint": "prettier --ignore-path .gitignore --check . && eslint --ignore-path .gitignore .",
    "preview": "vite preview",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "playwright test tests/e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.4",
    "@testing-library/jest-dom": "^6.1.1",
    "@testing-library/svelte": "^4.0.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/qrcode": "^1.5.2",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.0.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.33.2",
    "happy-dom": "^14.2.0",
    "postcss": "^8.4.35",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.2.1",
    "svelte": "^4.2.10",
    "svelte-check": "^3.2.0",
    "svelte-preprocess": "^5.0.2",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.4.1",
    "typescript": "~5.4.2",
    "vite": "^5.1.4",
    "vite-plugin-windicss": "^1.8.8",
    "vitest": "^1.1.1"
  },
  "type": "module",
  "dependencies": {
    "chartist": "^1.3.0",
    "qrcode": "^1.5.3",
    "svelte-confetti": "^1.3.0",
    "tailwindcss-animated": "^1.0.1"
  }
}
